{
  "name": "react-native-spaceviewer",
  "version": "1.0.0",
  "main": "App.js",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "appium": "appium",
    "run-android": "react-native run-android",
    "run-ios": "react-native run-ios --simulator 'iPhone 6s'",
    "test-ios": "E2E_DEVICE=local_ios jest",
    "test-android-debug": "E2E_DEVICE=local_android_debug jest",
    "test-android-release": "E2E_DEVICE=local_android_release jest",
    "build-android-debug": "cd android; ./gradlew assembleDebug",
    "build-android-release": "cd android; ./gradlew assembleRelease",
    "test-ios-debug": "E2E_DEVICE=local_ios_debug jest",
    "test-ios-release": "E2E_DEVICE=local_ios_release jest",
    "build-ios-debug": "cd ios; xcodebuild -scheme 'Space Viewer' -sdk iphonesimulator -configuration Debug -derivedDataPath build/app; zip -r app-debug.zip build/app/Build/Products/Debug-iphonesimulator/*.app",
    "build-ios-release": "cd ios; xcodebuild -scheme 'Space Viewer' -sdk iphonesimulator -configuration Release -derivedDataPath build/app; zip -r app-release.zip build/app/Build/Products/Release-iphonesimulator/*.app"
  },
  "dependencies": {
    "appcenter": "^1.7.1",
    "appcenter-analytics": "^1.7.1",
    "appcenter-crashes": "^1.7.1",
    "appcenter-push": "^1.7.1",
    "axios": "^0.18.0",
    "moment": "^2.22.2",
    "native-base": "^2.7.2",
    "react": "^16.5.2",
    "react-native": "^0.57.1",
    "react-native-code-push": "^5.4.2",
    "react-native-countdown-component": "^1.7.3",
    "react-native-fast-image": "^5.0.2",
    "react-native-google-static-map": "^0.1.12",
    "react-native-responsive-screen": "^1.1.7",
    "react-native-settings-list": "git://github.com/reime005/react-native-settings-list.git#expo",
    "react-native-vector-icons": "^5.0.0",
    "react-navigation": "1.1.2",
    "react-navigation-redux-helpers": "1.0.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "schedule": "^0.4.0",
    "venv": "^1.0.1",
    "watchman": "^1.0.0",
    "wd": "^1.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta",
    "appium": "^1.9.1",
    "appium-doctor": "^1.4.3",
    "babel-core": "^7.0.0-beta",
    "babel-jest": "23.4.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-native": "4.0.1",
    "jest": "^23.6.0",
    "metro-react-native-babel-preset": "0.45.6",
    "react-test-renderer": "16.5.2"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "setupFiles": [
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js",
      "<rootDir>/node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "<rootDir>/node_modules/appcenter-crashes/test/AppCenterCrashesMock.js",
      "<rootDir>/node_modules/appcenter-push/test/AppCenterPushMock.js"
    ]
  }
}